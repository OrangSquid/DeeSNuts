            "args": [
                "${workspaceFolder}\\bios\\BIOSGBA.ROM",
                "${workspaceFolder}\\tests\\arm.gba"
            ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "console": "externalTerminal"
        }
    ],
    "compounds": []
}